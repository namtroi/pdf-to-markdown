{
  "name": "pdf-to-markdown",
  "version": "1.0.0",
  "description": "A PDF to Markdown Converter",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check": "npm run type-check && npm run lint && npm run test",
    "preview": "vite preview"
  },
  "keywords": [
    "PDF",
    "Markdown",
    "Converter"
  ],
  "author": "Johannes Zillmann",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/namtroi/pdf-to-markdown"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "pdfjs-dist": "5.4",
    "postcss": "^8.5.6",
    "rc-progress": "^2.0.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.0.0",
    "react-icons": "^5.4.0",
    "remarkable": "^1.7.1",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@eslint/js": "^9.39.2",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
